@{
    Layout = "~/Views/Shared/_MpLayout.cshtml";
    ViewBag.Title = "汇通支付";
}

@section HeadSection
{
    <style>
        * {
            margin: 0;
            padding: 0;
            border: 0;
            outline: 0;
            border-radius: 0;
            -webkit-appearance: none;
            -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
            background: none;
        }

        body {
            padding: 15px;
        }

        .title {
            font-size: 18px;
            line-height: 2;
            padding: 15px 0;
            text-align: center;
            color: #333;
        }

        .label {
            display: -webkit-box;
            display: -webkit-flex;
            display: flex;
            font-size: 14px;
            line-height: 1.5;
            -webkit-box-pack: center;
            -webkit-justify-content: center;
            justify-content: center;
            -webkit-box-align: center;
            -webkit-align-items: center;
            align-items: center;
            margin-bottom: 15px;
        }

        .label_b,
        .label_t {
            font-size: 14px;
            line-height: 1.5;
        }

        .label_t {
            padding-right: 10px;
            color: #666;
        }

        .label_b {
            -webkit-box-flex: 1;
            -webkit-flex: 1;
            flex: 1;
            height: 29px;
            border: 1px solid #e5e5e5;
            padding: 3px 10px;
            box-sizing: content-box;
            border-radius: 4px;
        }

        .label select.label_b {
            background: url("data:image/png;base64,") no-repeat right center / 19px auto;
        }

        .label_f {
            padding-left: 10px;
        }

        .btn {
            display: block;
            width: 100%;
            padding: 10px 0;
            font-size: 16px;
            line-height: 25px;
            background: #09BB07;
            color: #fff;
            border-radius: 4px;
        }

            .btn:active {
                box-shadow: 0 0 50px rgba(0, 0, 0, 0.1) inset;
            }

        .message {
            font-size: 12px;
            line-height: 1.25;
            padding-top: 10px;
            display: block;
        }

        /*# sourceMappingURL=data:application/json;charset=utf8;base64, */
    </style>

}




<div class="title">《汇通支付》</div>
<div class="label">
    <span class="label_t">付款方式：</span>
    <select class="label_b" id="trade_type">
        <option value="weixin">微信扫码</option>
        <option value="bdpay">百度扫码支付</option>
        <option value="jdpay">京东扫码</option>
        <option value="tenpaywap">qq支付</option>
        <option value="qqmobile">qq扫码</option>
        <option value="alipay">支付宝扫码</option>
        <option value="alipaywap">支付宝wap</option>
        <option value="wxwap">微信wap</option>
        <option value="weixincode">微信付款码</option>
        <option value="alipaycode">支付宝付款码</option>
        <option value="yinlian">银联</option>
        <option value=" BCCB-NET-B2C">北京银行</option>
        <option value=" BEA-NET-B2C">东亚银行</option>
        <option value="ICBC-NET-B2C">工商银行</option>
        <option value="CEB-NET-B2C">光大银行</option>
        <option value="CGB-NET-B2C">广发银行</option>
        <option value="CCB-NET-B2C">建设银行</option>
        <option value="BOCOM-NET-B2C">交通银行</option>
        <option value=" CMBC-NET-B2C">民生银行</option>
        <option value="NJCB-NET-B2C">南京银行</option>
        <option value="NBCB-NET-B2C">宁波银行</option>
        <option value="ABC-NET-B2C">农业银行</option>
        <option value="PINGAN-NET-B2C">平安银行</option>
        <option value="SPDB-NET-B2C">浦发银行</option>
        <option value="SRCB-NET-B2C">上海农商行</option>
        <option value="CIB-NET-B2C">兴业银行</option>
        <option value="POST-NET-B2C">邮政储蓄银行</option>
        <option value="CMB-NET-B2C">招商银行</option>
        <option value="BOC-NET-B2C">中国银行</option>
        <option value="CITIC-NET-B2C">中信银行</option>
    </select>
</div>
<div class="label">
    <span class="label_t">存款金额：</span>
    <input type="text"
           class="label_b"
           placeholder="请输入存款金额"
           id="total_fee"
           onkeyup="this.value=this.value.replace(/\D/g, '')" />
    <span class="label_f"> 元</span>
</div>
<div>
    <input type="button" class="btn" value="提交" onclick="onPay();" />
 
</div>

<script>

    $('.btn').click(function () {
        var payType = $('#trade_type').val();
        var payAmount = $('#total_fee').val();
        if (!validate.isInteger(payAmount)) {
            OtherPay.MSG.alert("请输入正确的数字");
            return false;

        }
        if (payAmount <= 0 || payAmount > 100000000) {
            OtherPay.MSG.alert("请输入0.01-100000000范围内的金额");
            return false;
        }
        window.location.href = "/huitong/payto?payType=" + payType + "&payAmount=" + payAmount;
        //OtherPay.ACT.ajaxPost("/huihe/index", {
        //    payType: payType, payAmount: payAmount,
        //}, function (data) {
        //    if (data.Success) {

        //    }
        //    else {
        //        OtherPay.MSG.alert(data.Message);
        //    }
        //});
    })


</script>